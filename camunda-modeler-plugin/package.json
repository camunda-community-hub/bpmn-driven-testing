{
  "name": "bpmn-driven-testing-plugin",
  "version": "0.10.0-SNAPSHOT",
  "description": "Camunda Modeler Plugin for BPMN Driven Testing",
  "main": "index.js",
  "scripts": {
    "build": "webpack -c webpack.config.js",
    "coverage": "nyc --reporter=html mocha test --recursive --require @babel/register --colors",
    "dev": "webpack -c webpack.config.js -w",
    "test": "mocha test --recursive --require @babel/register --colors",
    "test-watch": "mocha test --recursive --require @babel/register --colors --watch --reporter min",
    "zip": "node zip.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "adm-zip": "^0.5.10",
    "babel-loader": "^8.3.0",
    "bpmn-moddle": "^8.1.0",
    "camunda-modeler-plugin-helpers": "5.1.0",
    "chai": "^4.4.1",
    "chai-spies": "^1.1.0",
    "css-loader": "^6.9.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
