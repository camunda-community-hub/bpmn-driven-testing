{
  "name": "bpmn-driven-testing-plugin",
  "version": "0.5.0-SNAPSHOT",
  "description": "Camunda Modeler Plugin for BPMN Driven Testing",
  "main": "index.js",
  "scripts": {
    "build": "webpack -c webpack.config.js",
    "coverage": "nyc --reporter=html mocha test --recursive --require @babel/register --colors",
    "dev": "webpack -c webpack.config.js -w",
    "test": "mocha test --recursive --require @babel/register --colors",
    "test-watch": "mocha test --recursive --require @babel/register --colors --watch --reporter min",
    "zip": "node zip.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.12.13",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "adm-zip": "^0.5.5",
    "babel-loader": "^8.1.0",
    "bpmn-moddle": "^7.0.4",
    "camunda-modeler-plugin-helpers": "3.4.0-alpha.0",
    "chai": "^4.3.0",
    "chai-spies": "^1.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "glob-parent": "5.1.2",
    "mocha": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
